<?xml version="1.0" encoding="utf-8"?>
<resources>
    <!--为CircleImageView设置的自定义属性-->
    <declare-styleable name="yc_circle_image_view">
        <attr name="border_width" format="dimension" />
        <attr name="border_color" format="color" />
        <attr name="border_overlay" format="boolean" />
        <attr name="civ_fill_color" format="color" />
    </declare-styleable>
    <!--为CircleImageView设置的自定义属性-->
    <declare-styleable name="yc_camera_sfv">
        <!--是否是后置摄像头-->
        <attr name="is_back_Camera" format="boolean"/>
    </declare-styleable>
</resources>
